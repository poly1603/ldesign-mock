{
  "name": "@ldesign/mock",
  "version": "1.0.0",
  "description": "强大的 Mock 数据管理工具，让前后端并行开发成为可能",
  "keywords": [
    "mock",
    "server",
    "faker",
    "graphql",
    "restful",
    "test",
    "development"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "ldesign-mock": "./bin/cli.js",
    "lmock": "./bin/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/mock-core": "workspace:*",
    "@ldesign/mock-server": "workspace:*",
    "commander": "^12.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.6.2",
    "ora": "^8.0.1",
    "boxen": "^7.1.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.16",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.0",
    "typescript": "^5.7.3",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

